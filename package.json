{
  "name": "@silintl/ui-components",
  "version": "3.2.0",
  "description": "Reusable Svelte components for some internal applications",
  "svelte": "dist/index.min.js",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo test",
    "format-all": "npx prettier --write .",
    "dev": "sass components/global.scss components/global.css; start-storybook -p 6007",
    "storybook": "sass components/global.scss components/global.css; build-storybook",
    "build": "rollup -c",
    "prepublishOnly": "npm run build"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/silinternational/ui-components.git"
  },
  "keywords": [
    "material",
    "material-design",
    "svelte",
    "svelte-components",
    "ui-components"
  ],
  "author": "Michael Wilson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/silinternational/ui-components/issues"
  },
  "homepage": "https://github.com/silinternational/ui-components#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-actions": "^6.4.21",
    "@storybook/addon-docs": "^6.4.21",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-svelte-csf": "^2.0.1",
    "@storybook/builder-webpack5": "^6.4.21",
    "@storybook/manager-webpack5": "^6.4.21",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/svelte": "^6.4.21",
    "css-loader": "^6.7.1",
    "material-components-web": "^13.0.0",
    "postcss": "^8.4.6",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "rollup": "^2.67.3",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "1.50.x",
    "sass-loader": "^12.6.0",
    "semantic-release": "^19.0.2",
    "style-loader": "^3.3.1",
    "svelte": "3.39.0",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.10.5"
  }
}
