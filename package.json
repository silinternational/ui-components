{
  "name": "@silintl/ui-components",
  "version": "3.10.1",
  "description": "Reusable Svelte components for some internal applications",
  "main": "index.js",
  "scripts": {
    "test": "echo test",
    "format-all": "npx prettier --write .",
    "dev": "sass components/global.scss components/global.css; start-storybook -p 6007",
    "build": "sass components/global.scss components/global.css; build-storybook"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/silinternational/ui-components.git"
  },
  "keywords": [
    "material",
    "material-design",
    "svelte",
    "svelte-components",
    "ui-components"
  ],
  "author": "Michael Wilson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/silinternational/ui-components/issues"
  },
  "homepage": "https://github.com/silinternational/ui-components#readme",
  "peerDependencies": {
    "@roxi/routify": "^2.x",
    "material-components-web": "^10.0.0",
    "svelte": "^3.x"
  },
  "devDependencies": {
    "@roxi/routify": "^2.x",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-docs": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-svelte-csf": "^1.1.0",
    "@storybook/builder-webpack5": "^6.3.2",
    "@storybook/manager-webpack5": "^6.4.0-alpha.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/svelte": "^6.3.2",
    "css-loader": "^5.2.6",
    "material-components-web": "^10.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "sass": "1.32.x",
    "sass-loader": "^11.0.1",
    "semantic-release": "^17.4.4",
    "style-loader": "^2.0.0",
    "svelte": "^3.x",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.7.3"
  }
}
